<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/background_image" />

    <!--<Button android:id="@+id/button1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Captured"/>-->
    <!--<Button android:id="@+id/button2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="MAP"/>-->
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="290dp"
        android:layout_height="147dp"
        android:src="@drawable/new_logo"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp" />

    <ImageView
        android:id="@+id/image_result"
        android:layout_width="243dp"
        android:layout_height="200dp"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="11dp" />

    <TextView
        android:id="@+id/gender"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_result"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="106dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="106dp"
        android:text=""
        android:textColor="#FFF" />

    <TextView
        android:id="@+id/age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gender"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="90dp"
        android:text=""
        android:textColor="#FFF" />

    <TextView
        android:id="@+id/result_survey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image_result"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="50dp"
        android:text=""
        android:textColor="#FFF" />

    <TextView
        android:id="@+id/smile"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/age"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="106dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="106dp"
        android:textColor="#FFF" />

    <!--<TextView-->
        <!--android:id="@+id/created"-->
        <!--android:layout_width="172dp"-->
        <!--android:layout_height="wrap_content"/>-->

    <!--<TextView-->
        <!--android:id="@+id/logged_in"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content" />-->

    <Button
        android:id="@+id/re_capture"
        android:layout_width="228dp"
        android:layout_height="31dp"
        android:layout_below="@id/smile"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_button"
        android:onClick="reCapture"
        android:text="@string/re_capture" />

    <Button
        android:id="@+id/search_main"
        android:layout_width="226dp"
        android:layout_height="30dp"
        android:layout_below="@id/re_capture"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="7dp"
        android:background="@drawable/rounded_button"
        android:text="@string/search_main"
        android:onClick="searchOnclick"/>


    <!--<Button-->
        <!--android:id="@+id/logout"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:onClick="logout"-->
        <!--android:text="Logout" />-->
    <!--android:text="CREATED BY FRANCIS DURANTE - POWERED BY MICROSOFT API" />-->

</RelativeLayout>